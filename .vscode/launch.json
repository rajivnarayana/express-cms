{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/express-sample/main.js",
            "stopOnEntry": false,
            "args": [
            ],
            "cwd": "${workspaceRoot}/express-sample",
            "preLaunchTask": null,
            "runtimeExecutable": null,
            "runtimeArgs": [
                "--nolazy"
            ],
            "env": {
                "NODE_ENV": "development",
                "MONGO_URL" : "mongodb://localhost/cms-sample-db"
            },
            "externalConsole": false,
            // "sourceMaps": true,
            "outDir": null
        },
        {
            "name": "Attach",
            "type": "node",
            "request": "attach",
            "port": 5858,
            "address": "localhost",
            "restart": false,
            "sourceMaps": false,
            "localRoot": "${workspaceRoot}/express-sample",
            "remoteRoot": "${workspaceRoot}/express-sample"
        }
    ]
}